{
  "name": "mctext-react",
  "version": "1.1.1",
  "description": "A React component to display Minecraft-formatted text.",
  "main": "src/index.js",
  "module": "src/index.js",
  "types": "src/index.d.ts",
  "exports": {
    ".": {
      "types": "./src/index.d.ts",
      "import": "./src/index.js",
      "require": "./src/index.js",
      "default": "./src/index.js"
    }
  },
  "files": [
    "lib",
    "src"
  ],
  "scripts": {
    "test": "standard && jest",
    "build": "babel src -d lib --ignore spec.js --copy-files",
    "prepublish": "babel src -d lib --ignore spec.js --copy-files",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TeamWertarbyte/mctext-react.git"
  },
  "keywords": [
    "react",
    "minecraft",
    "text",
    "component"
  ],
  "author": "Maik Marschner",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/TeamWertarbyte/mctext-react/issues"
  },
  "homepage": "https://github.com/TeamWertarbyte/mctext-react#readme",
  "devDependencies": {
    "@babel/cli": "^7.10.5",
    "@babel/core": "^7.11.4",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/preset-env": "^7.11.0",
    "@babel/preset-react": "^7.10.4",
    "@esbuild/linux-x64": "^0.25.9",
    "@storybook/addon-docs": "10.0.7",
    "@storybook/addon-webpack5-compiler-babel": "^4.0.0",
    "@storybook/react-vite": "10.0.7",
    "@storybook/react-webpack5": "10.0.7",
    "@types/node": "^24.3.0",
    "@types/react": "^19.1.10",
    "babel-eslint": "^7.1.1",
    "babel-jest": "^26.3.0",
    "jest": "^26.4.2",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-test-renderer": "^19.1.1",
    "rollup": "^4.47.1",
    "standard": "^8.6.0",
    "storybook": "10.0.7",
    "typescript": "^5.9.2",
    "vite": "^7.2.2",
    "@storybook/react": "^10.0.7",
  },
  "peerDependencies": {
    "react": "^19.1.0"
